{
  "manifest_version": 3,
  "name": "WordLearner",
  "version": "1.0",
  "permissions": ["activeTab", "identity", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "eua.png",
      "48": "eua.png",
      "128": "eua.png"
    }
  },
  "icons": {
    "16": "eua.png",
    "48": "eua.png",
    "128": "eua.png"
  }
}
